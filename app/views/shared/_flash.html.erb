<% unless Current.user.present? %>

    <% if flash[:notice]%>
        <div class="mt-20 bg-green-200 text-green-900 p-4 w-96" role="alert">
            <p><%= flash[:notice] %></p>
        </div>
    <% end %>

    <% if flash[:alert]%>
        <div class="mt-20 bg-red-200 text-red-800 p-4 w-96 m-0" role="alert">
            <p><%= flash[:alert] %></p>
        </div>
    <% end %>
<% end %>